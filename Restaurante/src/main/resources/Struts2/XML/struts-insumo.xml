<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="insumo" extends="json" namespace="/insumo">
        <action name="getModificar" method="getModificar" class="Acciones.InsumoAction"/>
        <action name="postModificar" method="postModificar" class="Acciones.InsumoAction"/>
        <action name="registrar" method="registrar" class="Acciones.InsumoAction"/>
        <action name="eliminar" method="eliminar" class="Acciones.InsumoAction"/>
        <action name="recuperar" method="recuperar" class="Acciones.InsumoAction"/>
        <action name="listar" method="listar" class="Acciones.InsumoAction">
            <result name="success" type="tiles">/insumo/todos.tiles</result>
        </action>
        <action name="nuevo" method="nuevo" class="Acciones.InsumoAction">
            <result name="success" type="tiles">/insumo/alta.tiles</result>
        </action>      
    </package>
</struts>